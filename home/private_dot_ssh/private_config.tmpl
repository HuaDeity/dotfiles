Host 2080
  HostName {{ onepasswordRead "op://Personal/2080/URL" }}
  User {{ onepasswordRead "op://Personal/2080/username" }}

Host Mastodon 
  HostName {{ onepasswordRead "op://Personal/DigitalOcean/URL" }}
  User {{ onepasswordRead "op://Personal/DigitalOcean/username" }}

Host 192.168.31.1
  HostName 192.168.31.1
  User root 
  HostKeyAlgorithms=+ssh-rsa

Host {{ onepasswordRead "op://Personal/NPU/port1" }}
  HostName {{ onepasswordRead "op://Personal/NPU/URL" }}
  User {{ onepasswordRead "op://Personal/NPU/username" }}
  Port {{ onepasswordRead "op://Personal/NPU/port1" }} 

Host {{ onepasswordRead "op://Personal/NPU/port2" }}
  HostName {{ onepasswordRead "op://Personal/NPU/URL" }}
  User {{ onepasswordRead "op://Personal/NPU/username" }}
  Port {{ onepasswordRead "op://Personal/NPU/port2" }} 

Host {{ onepasswordRead "op://Personal/NPU/port3" }}
  HostName {{ onepasswordRead "op://Personal/NPU/URL" }}
  User {{ onepasswordRead "op://Personal/NPU/username" }}
  Port {{ onepasswordRead "op://Personal/NPU/port3" }} 

{{- if ne .chezmoi.os "windows" }}
Host *
  IdentityAgent ~/.1password/agent.sock
{{- end }}