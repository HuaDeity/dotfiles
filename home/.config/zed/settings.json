{
  "theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "icon_theme": {
    "mode": "system",
    "light": "Catppuccin Latte",
    "dark": "Catppuccin Mocha"
  },
  "buffer_font_family": "Monaspace Neon Var",
  "buffer_font_fallbacks": ["Symbols Nerd Font Mono"],
  "buffer_font_features": {
    "calt": true,
    "liga": true,
    "ss01": true,
    "ss02": true,
    "ss03": true,
    "cv01": 2
  },
  "buffer_font_size": 13,
  "pane_split_direction_horizontal": "down",
  "pane_split_direction_vertical": "right",
  "vim_mode": true,
  "auto_signature_help": true,
  "title_bar": {
    "show_branch_icon": true
  },
  "minimap": {
    "show": "auto",
    "thumb": "hover",
    "thumb_border": "none"
  },
  "indent_guides": {
    "coloring": "indent_aware"
  },
  "vertical_scroll_margin": 1,
  "autoscroll_on_clicks": true,
  "horizontal_scroll_margin": 2,
  "use_smartcase_search": true,
  "inlay_hints": {
    "enabled": true
  },
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4.5"
    },
    "always_allow_tool_actions": true
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "tabs": {
    "git_status": false,
    "file_icons": true,
    "show_diagnostics": "all"
  },
  "format_on_save": "on",
  "diagnostics": {
    "inline": {
      "enabled": true
    }
  },
  "git": {
    "inline_blame": {
      "show_commit_summary": true
    }
  },
  "edit_predictions": {
    "mode": "subtle"
  },
  "terminal": {
    "option_as_meta": true,
    "copy_on_select": true,
    "detect_venv": {
      "on": {
        "activate_script": "fish"
      }
    }
  },
  "file_types": {
    "Ansible": [
      "**.ansible.yml",
      "**.ansible.yaml",
      "**/defaults/*.yml",
      "**/defaults/*.yaml",
      "**/meta/*.yml",
      "**/meta/*.yaml",
      "**/tasks/*.yml",
      "**/tasks/*.yaml",
      "**/handlers/*.yml",
      "**/handlers/*.yaml",
      "**/group_vars/*.yml",
      "**/group_vars/*.yaml",
      "**/playbooks/*.yaml",
      "**/playbooks/*.yml",
      "**playbook*.yaml",
      "**playbook*.yml"
    ],
    "Python constraints": ["*constraints*.txt"],
    "Python requirements": [
      "**/requirements/*.{in,txt}",
      "*requirements*.{in,txt}"
    ]
  },
  "auto_install_extensions": {
    "ansible": true,
    "basher": true,
    "biome": true,
    "catppuccin": true,
    "catppuccin-icons": true,
    "docker-compose": true,
    "dockerfile": true,
    "env": true,
    "fish": true,
    "git-firefly": true,
    "html": true,
    "just": true,
    "latex": true,
    "log": true,
    "ltex": true,
    "make": true,
    "markdown-oxide": true,
    "marksman": true,
    "neocmake": true,
    "python-requirements": true,
    "nix": true,
    "perplexity": true,
    "rainbow-csv": true,
    "ruby": true,
    "superhtml": true,
    "swift": true,
    "toml": true,
    "tree-sitter-query": true,
    "vue": true,
    "wakatime": true,
    "xml": true,
    "zig": true
  },
  "languages": {
    "C++": {
      "show_edit_predictions": false,
      "tab_size": 2
    },
    "Fish": {
      "formatter": {
        "external": {
          "command": "fish_indent"
        }
      }
    },
    "HTML": {
      "language_servers": ["vscode-html-language-server", "superhtml"],
      "formatter": {
        "language_server": {
          "name": "superhtml"
        }
      }
    },
    "JavaScript": {
      "prettier": {
        "allowed": false
      },
      "formatter": [
        {
          "code_action": "source.fixAll.biome"
        },
        {
          "code_action": "source.organizeImports.biome"
        },
        {
          "language_server": {
            "name": "biome"
          }
        }
      ]
    },
    "JSON": {
      "prettier": {
        "allowed": false
      },
      "formatter": [
        {
          "code_action": "source.fixAll.biome"
        },
        {
          "code_action": "source.organizeImports.biome"
        },
        {
          "language_server": {
            "name": "biome"
          }
        }
      ]
    },
    "JSONC": {
      "prettier": {
        "allowed": false
      },
      "formatter": [
        {
          "code_action": "source.fixAll.biome"
        },
        {
          "code_action": "source.organizeImports.biome"
        },
        {
          "language_server": {
            "name": "biome"
          }
        }
      ]
    },
    "Markdown": {
      "language_servers": ["marksman", "markdown-oxide"]
    },
    "Nix": {
      "language_servers": ["nixd", "!nil"],
      "formatter": {
        "external": {
          "command": "nixfmt",
          "arguments": ["--quiet", "--"]
        }
      }
    },
    "Python": {
      "language_servers": ["ty", "!basedpyright", "..."]
    },
    "Ruby": {
      "language_servers": ["ruby-lsp", "rubocop", "!solargraph"]
    },
    "Shell Script": {
      "formatter": {
        "external": {
          "command": "shfmt",
          "arguments": ["--filename", "{buffer_path}", "--indent", "2"]
        }
      },
      "tab_size": 2
    },
    "Swift": {
      "show_edit_predictions": false
    },
    "TypeScript": {
      "prettier": {
        "allowed": false
      },
      "formatter": [
        {
          "code_action": "source.fixAll.biome"
        },
        {
          "code_action": "source.organizeImports.biome"
        },
        {
          "language_server": {
            "name": "biome"
          }
        }
      ]
    }
  },
  "language_models": {
    "openai": {
      "api_url": "https://openrouter.ai/api/v1"
    },
    "anthropic": {
      "api_url": "https://aihubmix.com"
    }
  },
  "lsp": {
    "basedpyright": {
      "settings": {
        "basedpyright.analysis": {
          "typeCheckingMode": "off"
        },
        "basedpyright.disableOrganizeImports": true
      }
    },
    "protobuf-language-server": {
      "binary": {
        "path": "protols"
      }
    },
    "pyrefly": {
      "settings": {
        "python": {
          "pyrefly": {
            "disableTypeErrors": true
          }
        }
      }
    },
    "rubocop": {
      "initialization_options": {
        "safeAutocorrect": false
      },
      "settings": {
        "use_bundler": false
      }
    },
    "ruby-lsp": {
      "initialization_options": {
        "enabledFeatures": {
          "diagnostics": false
        }
      },
      "settings": {
        "use_bundler": false
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy"
        },
        "cargo": {
          "targetDir": true
        },
        "inlayHints": {
          "maxLength": null,
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "closureReturnTypeHints": {
            "enable": "always"
          }
        }
      }
    },
    "yaml-language-server": {
      "settings": {
        "yaml": {
          "keyOrdering": false
        }
      }
    }
  },
  "vim": {
    "toggle_relative_line_numbers": true,
    "use_smartcase_find": true
  },
